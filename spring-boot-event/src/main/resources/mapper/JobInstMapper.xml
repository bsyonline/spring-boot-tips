<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rolex.tips.mapper.JobInstMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.rolex.tips.model.JobInst">
        <id column="job_id" property="jobId" />
        <result column="job_name" property="jobName" />
    </resultMap>

    <select id="selectAll" resultType="com.rolex.tips.model.JobInst">
        select * from t_job_inst
    </select>

</mapper>
