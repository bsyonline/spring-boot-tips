<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rolex.tips.mapper.EventModelMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.rolex.tips.model.EventModel">
        <id column="event_id" property="eventId" />
        <result column="event_name" property="eventName" />
    </resultMap>

    <select id="selectAll" resultType="com.rolex.tips.model.EventModel">
        select * from t_event
    </select>
</mapper>
